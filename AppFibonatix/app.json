{
  "expo": {
    "name": "Shurtle",
    "slug": "fibonatixgameapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/LogoShurtle.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/LogoShurtle.png",
      "resizeMode": "contain",
      "backgroundColor": "#1f5100"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Esta aplicación necesita acceso a la cámara para escanear códigos QR."
      }
    },
    "android": {
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/TortugaJuego.png",
        "backgroundColor": "#1f5100"
      },
      "package": "com.emiliong.AppFibonatix",
      "newArchEnabled": true
    },
    "web": {
      "favicon": "./assets/img/TortugaJuego.png"
    },
    "plugins": [
      "expo-font",
      "expo-barcode-scanner"
    ],
    "extra": {
      "eas": {
        "projectId": "2987d78a-f723-49c9-8245-bd709d0a0828"
      }
    },
    "owner": "frognovainc"
  }
}
